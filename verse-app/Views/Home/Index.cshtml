@{
    ViewData["Title"] = "Home Page";
}
<div class="row">
    <div class="col-md-12 text-right">
        <strong id="Time">Time: 2:00</strong>
        <strong> | </strong>
        <strong id="Score">Score: 0</strong>
    </div>
</div>

<div class="row">
    <div class="col-md-12" id="Verse"></div>
</div>

<div class="row">
    <div class="col-md-12">
        <input type="text" id="Typed" />
        <input type="submit" class="btn btn-default" value="Submit" id="Submit" />
    </div>
</div>

<div class="row">
    <div class="col-md-12">
        <table class="table" id="History">
            <thead>
                <tr>
                    <th>Score</th>
                    <th>% Match</th>
                    <th>Verse</th>
                </tr>
            </thead>
            <tbody>

            </tbody>
        </table>
    </div>
</div>

<div id="StartPage">
    <input type="submit" class="btn btn-default" value="Start" id="Start" />
</div>

<div id="EndPage">
    <h3 id="FinalScore" class="text-center"></h3>
    <input type="submit" class="btn btn-default" value="Replay" id="Restart" />
</div>
